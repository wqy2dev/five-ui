{
	"name": "five-ui",
	"description": "FiveUI is an open source UI toolkit built with Svelte + Tailwind that allows you to create reactive web interfaces using Svelte + Tailwind CSS.",
	"version": "0.1.21-15",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wqy2dev/five-ui.git"
	},
	"author": {
		"name": "wqy2dev",
		"email": "wqyn@aliyun.com",
		"url": "https://github.com/wqy2dev"
	},
	"bugs": {
		"url": "https://github.com/wqy2dev/five-ui/issues"
	},
	"keywords": [
		"svelte",
		"sveltejs",
		"svelte5",
		"svelte-components",
		"component-library",
		"ui",
		"ui-components",
		"ui-kit"
	],
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"icon": "node scripts/icon.js"
	},
	"files": [
		"dist"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons/index.d.ts",
			"svelte": "./dist/icons/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.49.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"ace-builds": "^1.43.4",
		"publint": "^0.2.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-variants": "^0.3.0",
		"tailwindcss": "^3.4.18",
		"terser": "^5.44.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11"
	}
}
